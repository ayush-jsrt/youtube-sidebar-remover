{
    "manifest_version": 3,
    "name": "YouTube Sidebar Remover",
    "version": "1.0",
    "description": "Removes the sidebar (secondary div) on YouTube.",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }